#include <iostream>
#include <cmath>
using namespace std;
int main() {
	int num, n, number, res;
	res = 0;
	cin >> num;
	cin >> n;
	number = 0;
	while (num >= 1)
	{
		if (num % 10 != n) {
			number == 0 ? res += num % 10 : res += num % 10 * pow(10, number);
			number++;
		}
		num /= 10;
	}
	cout << res << endl;
	return 0;
}
